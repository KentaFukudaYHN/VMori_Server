services:
  mssql:
    container_name: mssql2019
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_COLLATION: Japanese_CI_AS
      MSSQL_LCID: '1041'
      MSSQL_PID: Express
      MSSQL_SA_PASSWORD: YEu23gA??WBYmNDN
    image: mcr.microsoft.com/mssql/rhel/server:2019-latest
    ports:
    - published: 1433
      target: 1433
    volumes:
    - /Users/kentafukuda/Documents/vmori/VMori_Server/docker/db/data:/var/opt/mssql/data:rw
    - /Users/kentafukuda/Documents/vmori/VMori_Server/docker/db/log:/var/opt/mssql/log:rw
    - /Users/kentafukuda/Documents/vmori/VMori_Server/docker/db/secrets:/var/opt/mssql/secrets:rw
  vmori:
    build:
      context: /Users/kentafukuda/Documents/vmori/VMori_Server
      dockerfile: ./src/VMori/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: VMori
      target: base
    container_name: VMori
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: vmori:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/VMori.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - /Users/kentafukuda/Documents/vmori/VMori_Server/src/VMori:/app:rw
    - /Users/kentafukuda/vsdbg/vs2017u5:/remote_debugger:rw
    - /Users/kentafukuda/.aspnet/https:/root/.aspnet/https:ro
    - /Users/kentafukuda/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
    - /Users/kentafukuda/.nuget/packages:/root/.nuget/packages:ro
    - /Users/kentafukuda/Documents/vmori/VMori_Server:/src:rw
version: '3.4'